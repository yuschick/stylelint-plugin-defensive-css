{
  "name": "stylelint-plugin-defensive-css",
  "version": "0.10.2",
  "description": "A Stylelint plugin to enforce defensive CSS best practices.",
  "main": "src/index.js",
  "files": [
    "src/**/*.js",
    "!**/**/*.test.js"
  ],
  "scripts": {
    "prepare": "husky install",
    "test": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuschick/stylelint-plugin-defensive-css.git"
  },
  "author": "Daniel Yuschick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yuschick/stylelint-plugin-defensive-css/issues"
  },
  "homepage": "https://github.com/yuschick/stylelint-plugin-defensive-css#readme",
  "engines": {
    "node": ">=18.12.0"
  },
  "keywords": [
    "Stylelint",
    "plugin",
    "CSS",
    "defensive CSS",
    "best practices"
  ],
  "peerDependencies": {
    "stylelint": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "husky": "^8.0.0",
    "jest": "^29.4.3",
    "jest-cli": "^29.4.3",
    "jest-preset-stylelint": "^7.0.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.1.2",
    "stylelint": "^16.0.2"
  },
  "lint-staged": {
    "**/*.{js|md}": [
      "eslint",
      "prettier --write"
    ]
  }
}
