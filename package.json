{
  "name": "stylelint-plugin-defensive-css",
  "version": "1.1.1",
  "description": "A Stylelint plugin to enforce defensive CSS best practices.",
  "main": "src/index.js",
  "type": "module",
  "exports": "./src/index.js",
  "files": [
    "src/**/*.js",
    "!**/**/*.test.js"
  ],
  "scripts": {
    "prepare": "husky install",
    "jest": "cross-env NODE_OPTIONS=\"--experimental-vm-modules\" jest --runInBand",
    "release": "semantic-release",
    "test": "npm run jest",
    "test:watch": "npm run jest -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuschick/stylelint-plugin-defensive-css.git"
  },
  "author": "Daniel Yuschick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yuschick/stylelint-plugin-defensive-css/issues"
  },
  "homepage": "https://github.com/yuschick/stylelint-plugin-defensive-css#readme",
  "engines": {
    "node": ">=18.12.0"
  },
  "keywords": [
    "css",
    "csslint",
    "defensive css",
    "lint",
    "linter",
    "stylelint",
    "stylelint-plugin"
  ],
  "peerDependencies": {
    "stylelint": "^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-cli": "^30.2.0",
    "jest-light-runner": "^0.7.11",
    "jest-preset-stylelint": "^9.1.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-eslint": "^16.4.2",
    "semantic-release": "^25.0.3",
    "stylelint": "^17.1.0"
  },
  "lint-staged": {
    "**/*.js|md|json": [
      "eslint",
      "prettier --write"
    ]
  }
}
